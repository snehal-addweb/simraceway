{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "severity": "warning"
      }
    ],
    "declaration-empty-line-before": null,
    "unit-whitelist": [
      "em",
      "rem",
      "ms",
      "px",
      "%",
      "deg"
    ]
  }
}
